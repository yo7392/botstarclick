from plyer import notification

xy = pyttsx3.init('sapi5')
voices = xy.getProperty('voices')
# print(voices[1].id)
xy.setProperty('voice', voices[1].id)
def speak(audio):
    xy.say(audio)
    xy.runAndWait()
